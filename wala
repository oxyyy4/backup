-- Scripts:

local function YFRCA_fake_script() -- Main.OMGANTICHEAT 
	local script = Instance.new('LocalScript', Main)

	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
	
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "Loaded!";
		Text = "Enjoy Made By Zonoxy#0003!";
		Duration = 3;
	})
	
end
coroutine.wrap(YFRCA_fake_script)()
