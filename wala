btn2.MouseButton1Down:Connect(function()
	local pp = game.Workspace["Ignored"]
	local titi = game.Workspace["MAP"]
	local pepe = game.Workspace["Players"]

	pp:Destroy()
	titi:Destroy()
	pepe:Destroy()
end)

btn1.MouseButton1Down:Connect(function()
